{
  "window.menuBarVisibility": "toggle",
  "editor.tabSize": 2,
  "editor.fontSize": 18,
  "editor.minimap.enabled": false,
  "editor.rulers": [80, 120],
  "editor.renderWhitespace": "all",
  "editor.scrollBeyondLastLine": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.autoReveal": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.record": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/*.pyc": true
  },
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "terminal.integrated.fontSize": 16,
  // "terminal.integrated.shell.linux": "/home/linuxbrew/.linuxbrew/bin/zsh",
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "eslint.validate": [
    "javascript",
    "vue"
  ],
  "todohighlight.toggleURI": true,
  "presentationMode.zoomLevel": 1,
  "vetur.format.scriptInitialIndent": true,
  "vetur.format.styleInitialIndent": true,
  "scss.lint.unknownProperties": "ignore",
  "scss.lint.emptyRules": "ignore",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "workbench.colorTheme": "Kiss",
  "liveshare.featureSet": "stable",
  "typescript.surveys.enabled": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "files.associations": {
    "*.json": "jsonc"
  },
  "git.enableSmartCommit": true,
  "git.autofetch": false,
  "settingsSync.ignoredExtensions": [
    "octref.vetur",
    "tlent.jest-snapshot-language-support",
    "ms-azuretools.vscode-docker",
    "mikestead.dotenv",
    "jock.svg",
    "dbaeumer.vscode-eslint",
    "formulahendry.github-actions"
  ]
}
